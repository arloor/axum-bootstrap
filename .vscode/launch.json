    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug  'basic'",
            "console": "integratedTerminal",
            "cargo": {
                "args": [
                    "build",
                    "--example=basic",
                ],
                "filter": {
                    "name": "basic",
                    "kind": "example"
                }
            },
            "args": [
                "--cert=cert.pem",
                "--key=privkey.pem",
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug  'jwt'",
            "console": "integratedTerminal",
            "cargo": {
                "args": [
                    "build",
                    "--example=jwt",
                    "--features=jwt"
                ],
                "filter": {
                    "name": "jwt",
                    "kind": "example"
                }
            },
            "args": [
                "--port=8888",
                "--username=admin",
                "--password=admin",
                "--jwt-secret=your_jwt_secret_key",
                "--cert=cert.pem",
                "--key=privkey.pem",
            ],
            "cwd": "${workspaceFolder}"
        },
    ]
}